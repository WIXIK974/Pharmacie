<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Label?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.pharmacix.MedicamentsController" spacing="10" alignment="CENTER">
    <Label text="Ajouter au stock d'un médicament" />
    <TextField fx:id="numMedicamentField" promptText="Numéro du médicament" />
    <TextField fx:id="quantiteAjouterField" promptText="Quantité à ajouter" />

    <Button text="Ajouter au stock" onAction="#ajouterStock" />

</VBox>
